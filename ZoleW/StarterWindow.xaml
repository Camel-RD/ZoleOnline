<Window x:Class="ZoleW.StarterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZoleW"
        mc:Ignorable="d"
        Title="Starter" Height="150" Width="150">

    <Canvas>
        <Button x:Name="btStartServer" Click="btStartServer_Click" Canvas.Left="18" Canvas.Top="25">
            Start Server
        </Button>
        <Button x:Name="btStart" Click="btStart_Click" Canvas.Left="18" Canvas.Top="68">
            Start new client
        </Button>
    </Canvas>
</Window>
